{
  "name": "japanese-tailwind-dictionary",
  "displayName": "Japanese Tailwind Dictionary",
  "description": "Tailwindのクラスを日本語訳したり、日本語で逆引き検索できるVSCode拡張機能。主要クラス網羅・日本語解説・右上アイコン・コマンドパレット対応。",
  "publisher": "LimeIvy",
  "icon": "images/title.png",
  "homepage": "https://github.com/LimeIvy/japanese-tailwind-dictionary",
  "repository": {
    "type": "git",
    "url": "https://github.com/LimeIvy/japanese-tailwind-dictionary.git"
  },
  "bugs": {
    "url": "https://github.com/LimeIvy/japanese-tailwind-dictionary/issues"
  },
  "keywords": [
    "tailwind",
    "css",
    "日本語",
    "japanese",
    "辞書",
    "dictionary",
    "翻訳",
    "translate",
    "vscode"
  ],
  "version": "0.1.2",
  "engines": {
    "vscode": "^1.60.0"
  },
  "categories": [
    "Other",
    "Programming Languages"
  ],
  "pricing": "Free",
  "activationEvents": [
    "onLanguage:javascript",
    "onLanguage:javascriptreact",
    "onLanguage:typescriptreact",
    "onLanguage:html",
    "onLanguage:vue",
    "onLanguage:svelte"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "read-tailwind.showClasses",
        "title": "Tailwind: 行のクラス詳細を表示"
      },
      {
        "command": "read-tailwind.reverseSearchClasses",
        "title": "Tailwind: CSS/日本語説明からクラスを逆引き検索",
        "icon": "images/icon.svg"
      }
    ],
    "menus": {
      "editor/title": [
        {
          "when": "editorLangId =~ /(javascript|javascriptreact|typescriptreact|html|vue|svelte)/",
          "command": "read-tailwind.reverseSearchClasses",
          "group": "navigation"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./"
  },
  "devDependencies": {
    "@types/node": "^16.x",
    "@types/vscode": "^1.60.0",
    "typescript": "^4.x"
  },
  "compilerOptions": {
    "module": "commonjs",
    "target": "ES2020",
    "outDir": "out",
    "lib": [
      "ES2020"
    ],
    "sourceMap": true,
    "rootDir": "src",
    "strict": true
  },
  "exclude": [
    "node_modules",
    ".vscode-test"
  ],
  "launch": {
    "version": "0.2.0",
    "configurations": [
      {
        "name": "拡張機能を実行",
        "type": "extensionHost",
        "request": "launch",
        "args": [
          "--extensionDevelopmentPath=${workspaceFolder}"
        ],
        "outFiles": [
          "${workspaceFolder}/out/**/*.js"
        ]
      }
    ]
  }
}
